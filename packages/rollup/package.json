{
  "name": "@kaivanwong/playgrounds-rollup",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@7.21.0",
  "description": "Rollup project for npm builds",
  "author": "Kaivan Wong <kaivanwong@outlook.com>",
  "license": "MIT",
  "homepage": "https://github.com/kaivanwong/playgrounds/packages/rollup#readme",
  "repository": {
    "url": "kaivanwong/playgrounds",
    "directory": "packages/rollup"
  },
  "bugs": {
    "url": "https://github.com/kaivanwong/playgrounds/issues"
  },
  "keywords": [
    "kaivanwong",
    "playgrounds",
    "rollup"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist",
    "*.d.ts",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "rollup -c ./rollup.config.js -w",
    "build": "rollup -c ./rollup.config.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@kaivanwong/eslint-config": "^0.12.8",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.2.1",
    "@rollup/plugin-typescript": "^10.0.1",
    "eslint": "^8.34.0",
    "rollup": "^3.15.0",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  }
}
